<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.HBox?>

<HBox prefHeight="118.0" prefWidth="160.0" spacing="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView id="image" fitHeight="53.0" fitWidth="73.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/Food01.png" />
         </image>
      </ImageView>
      <VBox prefHeight="122.0" prefWidth="116.0" spacing="10.0">
         <children>
            <Label id="name" text="모델명" />
            <ImageView id="scoreImage" pickOnBounds="true" preserveRatio="true" VBox.vgrow="ALWAYS">
               <image>
                  <Image url="@images/star1.png" />
               </image>
            </ImageView>
            <Label id="description" text="설명내용" wrapText="true" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </VBox>
   </children>
</HBox>
